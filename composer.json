{
	"name" : "tactfactory/dory-bundle",
	"type" : "symfony-bundle",
	"description" : "TACTfactory Project DRY",
	"authors" : [{
			"name" : "Jonathan Poncy",
			"email" : "poncy.jonathan@gmail.com"
		}, {
			"name" : "Mickael Gaillard",
			"email" : "mickael.gaillard@tactfactory.com"
		}
	],
	"keywords" : [
		"DRY"
	],
	"autoload" : {
		"psr-4" : {
			"Tact\\DoryBundle\\" : "",
			"Tact\\DoryBundle\\Composer\\" : ""
		}
	},
	"require" : {
		"php" : "^7.3",
		"symfony/symfony" : "3.4.*",
		"doctrine/orm": "^2.2",
		"doctrine/doctrine-bundle": "^1.10",
		"symfony/swiftmailer-bundle": "^3.2",
		"symfony/monolog-bundle": "^3.3",
		"sensio/distribution-bundle": "^5.0",
		"sensio/framework-extra-bundle": "^5.2",
		"incenteev/composer-parameter-handler": "^2.1",
		"doctrine/doctrine-fixtures-bundle": "^3.1",
		"friendsofsymfony/rest-bundle": "^2.5",

		"jms/serializer-bundle": "^1.0 || ^2.0",
		"nelmio/api-doc-bundle": "^3.4",
		"lexik/maintenance-bundle": "^2.1",
		"sonata-project/admin-bundle": "^3.48",
		"sonata-project/doctrine-orm-admin-bundle": "^3.8",
		"sonata-project/user-bundle": "^4.3",
		"sonata-project/media-bundle": "^3.19",
		"sonata-project/intl-bundle": "^2.5",
		"doctrine/migrations": "^2.0",
		"doctrine/doctrine-migrations-bundle": "^2.0",
		"willdurand/faker-bundle": "^1.4",
		"ob/highcharts-bundle": "^1.6",
		"knplabs/knp-paginator-bundle": "^3.0",
		"lexik/form-filter-bundle": "^5.0",
		"ornicar/gravatar-bundle": "^1.3",

		"knplabs/knp-gaufrette-bundle": "^0.5.3",
		"liip/imagine-bundle": "^2.1",
		"cocur/slugify": "^3.2"
	},
	"require-dev" : {
		"sensio/generator-bundle" : "~3.0",
		"symfony/phpunit-bridge" : "~2.7",
		"liip/functional-test-bundle" : "^1.6"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
			"Tact\\Bundle\\DoryBundle\\script\\ScriptHandler::install"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
			"Tact\\Bundle\\DoryBundle\\script\\ScriptHandler::install"
		]
	},
	"config" : {
		"bin-dir" : "bin",
		"platform" : {
			"php" : "7.3.2"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.2-dev"
		}
	},
	"minimum-stability" : "stable",
	"homepage" : "https://support.tactfactory.com/projects/tact-dory-bundle-web",
	"support" : {
		"issues" : "https://support.tactfactory.com/projects/tact-dory-bundle-web/issues",
		"wiki" : "https://support.tactfactory.com/projects/tact-dory-bundle-web/wiki",
		"source" : "http://git.tactfactory.com/tact-dory-bundle/tact-dory-bundle-web.git"
	},
	"license" : "proprietary"
}
