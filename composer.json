{
	"name" : "tactfactory/dory-bundle",
	"type" : "symfony-bundle",
	"description" : "TACTfactory Project DRY",
	"authors" : [{
			"name" : "Jonathan Poncy",
			"email" : "poncy.jonathan@gmail.com"
		}, {
			"name" : "Mickael Gaillard",
			"email" : "mickael.gaillard@tactfactory.com"
		}
	],
	"keywords" : [
		"DRY"
	],
	"autoload" : {
		"psr-4" : {
			"Tact\\DoryBundle\\" : ""
		}
	},
	"require" : {
		"php" : "^5.3 || ^7.0",
		"symfony/symfony" : "2.8.*",
		"doctrine/orm" : "^2.4.8",
		"doctrine/doctrine-bundle" : "~1.4",
		"symfony/swiftmailer-bundle" : "~2.3",
		"symfony/monolog-bundle" : "~2.4",
		"sensio/distribution-bundle" : "~5.0",
		"sensio/framework-extra-bundle" : "^3.0.2",
		"incenteev/composer-parameter-handler" : "~2.0",
		"doctrine/doctrine-fixtures-bundle" : "^2.3 || ^3.0",
		"friendsofsymfony/rest-bundle" : "^1.8 || ^2.0",
		"friendsofsymfony/oauth-server-bundle" : "^1.5",
		"jms/serializer-bundle" : "^0.13 || ^1.0",
		"nelmio/api-doc-bundle" : "^2.13",
		"lexik/maintenance-bundle" : "^2.1",
		"sonata-project/admin-bundle" : "^2.3 || ^3.0",
		"sonata-project/doctrine-orm-admin-bundle" : "^2.3 || ^3.0",
		"sonata-project/user-bundle" : "^2.3 || ^3.0",
		"sonata-project/datagrid-bundle" : "^2.3 || ^3.0",
		"sonata-project/easy-extends-bundle" : "^2.3 || ^3.0",
		"sonata-project/media-bundle" : "^2.3 || ^3.0",
		"sonata-project/intl-bundle" : "^2.3 || ^3.0",
		"doctrine/migrations" : "^1.4",
		"doctrine/doctrine-migrations-bundle" : "^1.1",
		"willdurand/faker-bundle" : "^1.3",
		"ob/highcharts-bundle" : "^1.4",
		"knplabs/knp-paginator-bundle" : "^2.5",
		"lexik/form-filter-bundle" : "^5.0",
		"ornicar/gravatar-bundle" : "^1.1",
		"vich/uploader-bundle" : "^1.1",
		"knplabs/knp-gaufrette-bundle" : "^0.3",
		"liip/imagine-bundle" : "^1.5",
		"cocur/slugify" : "^2.1"
	},
	"require-dev" : {
		"sensio/generator-bundle" : "~3.0",
		"symfony/phpunit-bridge" : "~2.7",
		"liip/functional-test-bundle" : "^1.6",
		"coresphere/console-bundle" : "^0.4"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
		]
	},
	"config" : {
		"bin-dir" : "bin",
		"platform" : {
			"php" : "7.0.7"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.1-dev"
		}
	},
	"minimum-stability" : "dev",
	"homepage" : "https://support.tactfactory.com/projects/management-projects-web",
	"support" : {
		"issues" : "https://support.tactfactory.com/projects/management-projects-web/issues",
		"wiki" : "https://support.tactfactory.com/projects/management-projects-web/wiki",
		"source" : "http://git.tactfactory.com/management-projects/management-projects-web.git"
	},
	"license" : "proprietary"
}